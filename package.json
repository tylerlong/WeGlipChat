{
  "name": "we-glip-chat",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "author": "Tyler Long <tyler4long@gmail.com>",
  "scripts": {
    "upgrade": "yarn-upgrade-all && yarn remove framework7 framework7-vue && yarn add framework7@2.3.1 framework7-vue@2.3.0",
    "prepush": "standard --plugin html '**/*.{js,vue}'",
    "build": "cp src/*.html build/ && cp src/*.png build/ && webpack --progress --colors --config webpack.dev.babel.js",
    "release": "rm -rf docs && mkdir docs && cp src/*.html docs/ && cp src/*.png docs/ && webpack --progress --colors --config webpack.prod.babel.js"
  },
  "dependencies": {
    "framework7": "2.3.1",
    "framework7-vue": "2.3.0",
    "js-cookie": "^2.2.0",
    "ramda": "^0.25.0",
    "ringcentral-js-concise": "^0.4.3",
    "urijs": "^1.19.1",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^0.28.11",
    "eslint-plugin-html": "^4.0.3",
    "husky": "^0.14.3",
    "mini-css-extract-plugin": "^0.4.0",
    "standard": "^11.0.1",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.7",
    "yarn-upgrade-all": "^0.3.0"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "standard": {
    "ignore": [
      "/docs/"
    ]
  }
}
